{
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "moduleResolution": "node",
        "declaration": false,
        "noImplicitAny": false,
        "sourceMap": true,
        "jsx": "react",
        "noLib": false,
        "suppressImplicitAnyIndexErrors": true,
        "types": [
            "webpack-env",
            "karma-chai-sinon",
            "mocha"
        ]
    },
    "compileOnSave": false,
    "exclude": [
        "node_modules"
    ]
}